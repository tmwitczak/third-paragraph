// //////////////////////////////////////////////////////// GLSL version //
#version 430 core

// ////////////////////////////////////////////////////////////// Inputs //
in vec2 texCoordF;

// ///////////////////////////////////////////////////////////// Outputs //
out vec4 outColor;

// //////////////////////////////////////////////////////////// Uniforms //
uniform sampler2D texture0;

// //////////////////////////////////////////////////////////////// Main //
void main() {
    outColor = texture(texture0, texCoordF);
}

// ///////////////////////////////////////////////////////////////////// //
